                    }
                },
                "400": {
                    "description": "",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "errors": {
                                        "type": "array",
                                        "items": {
                                            "$ref": "../../components/schemas.json#/error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "security": [
                {
                    "Authorization": []
                }
